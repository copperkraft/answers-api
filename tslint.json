{
  "rules": {
    "class-name": true,
    "curly": true,
    "eofline": true,
    "label-position": true,
    "label-undefined": true,
    "no-consecutive-blank-lines": true,
    "no-empty": true,
    "no-eval": true,
    "no-unreachable": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "triple-equals": [true, "allow-null-check"],
    "variable-name": [true, "check-format"]
  }
}
